{
    "type": "module",
    "scripts": {
        "dev": "concurrently \"pnpm:watch:build\" \"pnpm:preview\"",
        "build": "tsx src/index.tsx",
        "watch:build": "chokidar './src/**/*' './public/**/*' '../ensips/*.md' -t 500 --initial -c 'pnpm build'",
        "preview": "pnpm run build && pnpm vite preview",
        "lint": "eslint -c .eslintrc.json --ext .ts,.tsx ./src"
    },
    "dependencies": {
        "@adraffy/ens-normalize": "^1.11.0",
        "@shikijs/rehype": "^3.3.0",
        "cssnano": "^7.0.5",
        "gray-matter": "^4.0.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rehype-stringify": "^10.0.1",
        "remark": "^15.0.1",
        "remark-frontmatter": "^5.0.0",
        "remark-gfm": "^4.0.1",
        "remark-html": "^16.0.1",
        "remark-parse": "^11.0.0",
        "remark-rehype": "^11.1.2",
        "tsx": "^4.19.0",
        "unified": "^11.0.5",
        "unist": "^0.0.1",
        "vite": "^5.4.3",
        "yaml": "^2.5.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^22.5.2",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/unist": "^3.0.3",
        "@typescript-eslint/parser": "^8.4.0",
        "autoprefixer": "^10.4.20",
        "chokidar-cli": "^3.0.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.48.0",
        "eslint-plugin-v3xlabs": "^1.6.5",
        "postcss": "^8.4.44",
        "postcss-cli": "^11.0.0",
        "postcss-nested": "^6.2.0",
        "tailwindcss": "^3.4.10",
        "typescript": "^5.5.4"
    },
    "packageManager": "pnpm@10.18.2",
    "engines": {
        "node": ">=18",
        "pnpm": ">=10"
    }
}
